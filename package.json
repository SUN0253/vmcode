{
  "name": "vmcode",
  "displayName": "vmcode",
  "description": "Show hover information for ASM files",
  "version": "0.0.1",
  "publisher": "ikun909",
  "icon": "image/asm.png",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
     "onLanguage:asm"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vmcode.helloWorld",
        "title": "Hello IKUN"
      }
    ],
    "languages": [
      {
        "id": "asm",
        "extensions": [
          ".asm"
        ],
        "aliases": [
          "ASM"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.97.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
